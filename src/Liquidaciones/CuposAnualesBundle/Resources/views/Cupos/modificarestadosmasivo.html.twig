{# empty Twig template #}
{% extends '::base.html.twig' %}


{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/jquery.dataTables.1.9.js') }}" type="text/javascript"></script>
    <script src="{{ asset('js/dataTables.bootstrap.js') }}" type="text/javascript"></script>
    <!-- some extra scripts -->    

    
{% endblock %}

{% block guia %}
    {{ parent() }}           
    <li><a href="{{ path('cupos') }}">Cupos Mensuales</a></li>
    <li class="active">Modificar Estados</li>
{% endblock %}                
                
{% block contenido %}
<style>
    .grid-column-cupos_monto {
        text-align: right;
    }

</style>

<br />

                        

<div class="row">
    <h4><span class="glyphicon glyphicon-info-sign"></span> Recuerde que modificará masivamente el estado de los cupos.</h4> 
</div>

<hr>

<div class="row">
    <h4><span class="glyphicon glyphicon-search"></span> Resultado de la búsqueda:</h4>
    <div class="e10"></div>
    {{grid(grid, _self)}}
    {% block grid_filters %}{% endblock %}
</div>


{% endblock %}
