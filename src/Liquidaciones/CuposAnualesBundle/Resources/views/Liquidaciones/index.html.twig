

{% block body -%}
    <h1>Liquidaciones list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Refcupotipoliquidacion</th>
                <th>Idpersonalcargo</th>
                <th>Idconcepto</th>
                <th>Hsexvalorhora</th>
                <th>Hsexcantsimples</th>
                <th>Hsexcantdobles</th>
                <th>Rgidpersonalcargo</th>
                <th>Rgfecha</th>
                <th>Rgcanthsguardia</th>
                <th>Rgidnovedad</th>
                <th>Montototalcalculado</th>
                <th>Requiereautorizacion</th>
                <th>Usuaautoriza</th>
                <th>Usuacrea</th>
                <th>Fechacrea</th>
                <th style="width: 150px;">Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('liquidaciones_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.refCupoTipoLiquidacion }}</td>
                <td>{{ entity.idPersonalCargo }}</td>
                <td>{{ entity.idConcepto }}</td>
                <td>{{ entity.hsExValorHora }}</td>
                <td>{{ entity.hsExCantSimples }}</td>
                <td>{{ entity.hsExCantDobles }}</td>
                <td>{{ entity.rGIdPersonalCargo }}</td>
                <td>{% if entity.rGFecha %}{{ entity.rGFecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.rGCantHsGuardia }}</td>
                <td>{{ entity.rGIdNovedad }}</td>
                <td>{{ entity.montoTotalCalculado }}</td>
                <td>{{ entity.requiereAutorizacion }}</td>
                <td>{{ entity.usuaAutoriza }}</td>
                <td>{{ entity.usuaCrea }}</td>
                <td>{% if entity.fechaCrea %}{{ entity.fechaCrea|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td style="width: 120px;">
                <ul>
                    <li>
                        <a href="{{ path('liquidaciones_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('liquidaciones_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('liquidaciones_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
